<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.view.SceneController">
   <children>
      <AnchorPane prefHeight="355.0" prefWidth="720.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="level" layoutX="282.0" layoutY="14.0" text="Choose level">
               <font>
                  <Font size="28.0" />
               </font></Label>
            <Button fx:id="easyButton" layoutX="339.0" layoutY="100.0" mnemonicParsing="false" onAction="#switchToSudokuBoardScene" text="Easy" />
            <Button fx:id="mediumButton" alignment="CENTER" layoutX="328.0" layoutY="165.0" mnemonicParsing="false" text="Medium" />
            <Button fx:id="hardButton" layoutX="337.0" layoutY="236.0" mnemonicParsing="false" text="Hard" />
         </children></AnchorPane>
   </children>
</AnchorPane>
